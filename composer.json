{
    "name": "romegadigital/multitenancy-nova-tool",
    "description": "A Laravel Nova tool to manage multitenancy.",
    "keywords": [
        "laravel",
        "nova",
        "multitenancy"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "romegadigital/multitenancy": "^4.0",
        "kiritokatklian/nova-permission": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RomegaDigital\\MultitenancyNovaTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RomegaDigital\\MultitenancyNovaTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories":[
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
